import face_recognition
import os
import cv2

image = face_recognition.load_image_file("Ryan_1.Jpg")
face_locations = face_recognition.face_locations(image)

face_location = face_locations[0] 
face_encodings = face_recognition.face_encodings(image, [face_location])
Ryan_R_knwon_face_encoding_1 = face_encodings[0]

image = face_recognition.load_image_file("Ryan_2.Jpg")
face_locations = face_recognition.face_locations(image)

face_location = face_locations[0]
face_encodings = face_recognition.face_encodings(image, [face_location])
Ryan_R_knwon_face_encoding_2 = face_encodings[0]

image = face_recognition.load_image_file("Ryan_3.Jpg")  
unknwon_face_encodings = face_recognition.face_encodings(image)

print('Matches for Ryan R')
for unknwon_face_encoding in unknwon_face_encodings:
    matches = face_recognition.compare_faces(
        [Ryan_R_knwon_face_encoding_1, Ryan_R_knwon_face_encoding_2], 
        unknwon_face_encoding 
    )
    print(matches)
